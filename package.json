{
    "name": "graph-elements",
    "displayName": "graph-elements",
    "description": "",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.104.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "graph-elements.openEditorPage",
                "title": "Open Editor Page"
            }
        ],
        "customEditors": [
            {
                "viewType": "graph-elements.graph-editor",
                "displayName": "Graph Editor",
                "selector": [
                    {
                        "filenamePattern": "*.json"
                    }
                ],
                "priority": "default"
            }
        ]
    },
    "files": [
        "out/**/*",
        "resources/**/*"
    ],
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "sh compile.sh",
        "watch": "tsc -watch -p ./",
        "pretest": "npm run compile && npm run lint",
        "lint": "eslint src",
        "test": "vscode-test"
    },
    "devDependencies": {
        "@babel/preset-env": "^7.28.3",
        "@types/mocha": "^10.0.10",
        "@types/node": "22.x",
        "@types/vscode": "^1.104.0",
        "@typescript-eslint/eslint-plugin": "^8.45.0",
        "@typescript-eslint/parser": "^8.45.0",
        "@vscode/test-cli": "^0.0.11",
        "@vscode/test-electron": "^2.5.2",
        "babel": "^5.8.38",
        "babel-core": "^6.26.3",
        "babel-loader": "^10.0.0",
        "eslint": "^9.36.0",
        "inliner": "^1.13.1",
        "less": "^4.4.2",
        "less-loader": "^12.3.0",
        "typescript": "^5.9.3",
        "vite": "^7.1.11",
        "webpack": "^5.102.1",
        "webpack-cli": "^6.0.1"
    }
}
