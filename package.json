{
    "name": "graph-editor",
    "displayName": "graph-editor",
    "description": "",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.104.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "graph-editor.addNode",
                "title": "Add Node"
            },
            {
                "command": "graph-editor.deleteNode",
                "title": "Delete Node"
            },
            {
                "command": "graph-editor.startEulerLayout",
                "title": "Start Physics Layout"
            },
            {
                "command": "graph-editor.startKlayLayout",
                "title": "Start DAG Layout"
            }
        ],
        "customEditors": [
            {
                "viewType": "graph-editor",
                "displayName": "Graph Editor",
                "selector": [
                    {
                        "filenamePattern": "*.json"
                    }
                ],
                "priority": "default"
            }
        ],
        "menus": {
            "webview/context": [
                {
                    "when": "!graph-editor.node-selected",
                    "command": "graph-editor.addNode"
                },
                {
                    "group": "7_modification",
                    "when": "graph-editor.node-selected",
                    "command": "graph-editor.deleteNode"
                },
                {
                    "group": "7_modification",
                    "submenu": "graph-editor.layout"
                }
            ],
            "graph-editor.layout": [
                {
                    "when": "!graph-editor.node-selected",
                    "command": "graph-editor.startEulerLayout",
                    "submenu": "graph-editor.layout"
                },
                {
                    "when": "!graph-editor.node-selected",
                    "command": "graph-editor.startKlayLayout",
                    "submenu": "graph-editor.layout"
                }
            ]
        },
        "submenus": [
            {
                "id": "graph-editor.layout",
                "label": "Layout"
            }
        ]
    },
    "files": [
        "out/**/*",
        "resources/**/*"
    ],
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "sh compile.sh",
        "run": "sh compile.sh && tsc -p ./",
        "pretest": "npm run compile && npm run lint",
        "lint": "eslint src",
        "test": "vscode-test"
    },
    "devDependencies": {
        "@babel/preset-env": "^7.28.6",
        "@types/mocha": "^10.0.10",
        "@types/node": "25.x",
        "@types/vscode": "^1.108.1",
        "@typescript-eslint/eslint-plugin": "^8.53.0",
        "@typescript-eslint/parser": "^8.53.0",
        "@vscode/test-cli": "^0.0.12",
        "@vscode/test-electron": "^2.5.2",
        "babel": "^6.23.0",
        "babel-core": "^6.26.3",
        "babel-loader": "^10.0.0",
        "eslint": "^9.39.2",
        "inline-source": "^8.0.3",
        "inline-source-cli": "^2.0.0",
        "typescript": "^5.9.3",
        "typescript-cli": "^0.1.0",
        "webpack": "^5.104.1",
        "webpack-cli": "^6.0.1",
        "cytoscape-euler": "^1.2.3",
        "cytoscape-klay": "^3.1.4"
    }
}