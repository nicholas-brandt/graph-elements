{
    "name": "graph-editor",
    "displayName": "graph-editor",
    "description": "",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.104.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "graph-editor.addNode",
                "title": "Add Node"
            },
            {
                "command": "graph-editor.deleteNode",
                "title": "Delete Node"
            }
        ],
        "customEditors": [
            {
                "viewType": "graph-editor",
                "displayName": "Graph Editor",
                "selector": [
                    {
                        "filenamePattern": "*.json"
                    }
                ],
                "priority": "default"
            }
        ],
        "menus": {
            "webview/context": [
                {
                    "group": "7_modification",
                    "when": "!graph-editor.node-selected",
                    "command": "graph-editor.addNode"
                },
                {
                    "group": "7_modification",
                    "when": "graph-editor.node-selected",
                    "command": "graph-editor.deleteNode"
                },
                {
                    "command": "editor.action.clipboardCutAction",
                    "group": "5_cutcopypaste",
                    "when": "false"
                },
                {
                    "command": "editor.action.clipboardCopyAction",
                    "group": "5_cutcopypaste",
                    "when": "false"
                },
                {
                    "command": "editor.action.clipboardCutAction",
                    "when": "false"
                },
                {
                    "command": "editor.action.clipboardCopyAction",
                    "when": "false"
                }
            ],
            "editor/context": [
                {
                    "command": "editor.action.clipboardCutAction",
                    "group": "5_cutcopypaste",
                    "when": "false"
                },
                {
                    "command": "editor.action.clipboardCopyAction",
                    "group": "5_cutcopypaste",
                    "when": "false"
                }
            ]
        }
    },
    "files": [
        "out/**/*",
        "resources/**/*"
    ],
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "sh compile.sh",
        "watch": "tsc -p ./",
        "pretest": "npm run compile && npm run lint",
        "lint": "eslint src",
        "test": "vscode-test"
    },
    "devDependencies": {
        "@babel/preset-env": "^7.28.3",
        "@types/mocha": "^10.0.10",
        "@types/node": "22.x",
        "@types/vscode": "^1.104.0",
        "@typescript-eslint/eslint-plugin": "^8.45.0",
        "@typescript-eslint/parser": "^8.45.0",
        "@vscode/test-cli": "^0.0.11",
        "@vscode/test-electron": "^2.5.2",
        "babel": "^5.8.38",
        "babel-core": "^6.26.3",
        "babel-loader": "^10.0.0",
        "eslint": "^9.36.0",
        "inline-source": "^8.0.3",
        "inline-source-cli": "^2.0.0",
        "typescript": "^5.9.3",
        "typescript-cli": "^0.1.0",
        "webpack": "^5.102.1",
        "webpack-cli": "^6.0.1"
    }
}